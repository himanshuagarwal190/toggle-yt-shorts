{
    "name": "Toggle YT Shorts/Videos",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
      "tabs"
    ],
    "content_scripts": [
        {
          "matches": ["https://www.youtube.com/*"],
          "js": ["script.js"],
          "css": ["styles.css"],
          "run_at": "document_end"
        }
    ]
  }